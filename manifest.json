{
"manifest_version": 2,
    "name": "Solid Citizen Internet Enhancer",
    "description": "Turn your weak browsing experience into a solid one.",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ],
	"icons": { 
		"16": "i/icon32.png",
		"32": "i/icon64.png",
		"64": "i/icon128.png",
		"128": "i/icon256.png"
	}
}